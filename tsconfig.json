    "module": "NodeNext",
    "rootDir": "./src",
    "outDir": "./dist/assets/scripts",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules"]
}
